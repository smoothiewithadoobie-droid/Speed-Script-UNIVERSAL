        ownerBtn.MouseButton1Click:Connect(function() createTag("[OWNER] " .. player.Name, Color3.fromRGB(255, 80, 80)); broadcastRank("OWNER") end)
        coOwnerBtn.MouseButton1Click:Connect(function() createTag("[CO-OWNER] " .. player.Name, Color3.fromRGB(255, 200, 80)); broadcastRank("CO-OWNER") end)
