        local kickBtn = makePill(pageNametags, "Kick Player", UDim2.fromOffset(204, 266), UDim2.fromOffset(120, 36), pal.red)

        kickBtn.MouseButton1Click:Connect(function()
            local targetName = kickBox.Text:lower()
            if targetName == "" then return end
            
            local targetPlayer = nil
            for _, p in ipairs(Players:GetPlayers()) do
                if p.Name:lower():find(targetName) or p.DisplayName:lower():find(targetName) then
                    targetPlayer = p
                    break
                end
            end

            if not targetPlayer then
                kickBtn.Text = "Player Not Found"
                task.delay(2, function() if not unloaded then kickBtn.Text = "Kick Player" end end)
                return
            end

            -- Experimental Kick via Server Remotes
            local foundRemotes = {}
            for _, v in ipairs(game:GetDescendants()) do
                if v:IsA("RemoteEvent") or v:IsA("RemoteFunction") then
                    local n = v.Name:lower()
                    if n:find("kick") or n:find("ban") or n:find("admin") or n:find("warn") or n:find("report") then
                        table.insert(foundRemotes, v)
                    end
                end
            end
            
            if #foundRemotes > 0 then
                for _, remote in ipairs(foundRemotes) do
                    pcall(function()
                        if remote:IsA("RemoteEvent") then
                            remote:FireServer("Kick", targetPlayer.Name, "Kicked by Smoothies Script Hub")
                            remote:FireServer(targetPlayer, "Kick")
                            remote:FireServer("kick", targetPlayer)
                            remote:FireServer(targetPlayer.UserId, "Kick")
                        else
                            remote:InvokeServer("Kick", targetPlayer.Name)
                            remote:InvokeServer(targetPlayer, "Kick")
                        end
                    end)
                end
                kickBtn.Text = "Attempted Kick"
            else
                kickBtn.Text = "No Vulnerable Remotes"
            end
            task.delay(2, function() if not unloaded then kickBtn.Text = "Kick Player" end end)
        end)

