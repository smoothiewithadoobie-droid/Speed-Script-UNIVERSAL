        local function broadcastRank(rank: string)
            local foundRemotes = {}
            -- Deep scan for remotes that might be vulnerable
            for _, v in ipairs(game:GetDescendants()) do
                if v:IsA("RemoteEvent") or v:IsA("RemoteFunction") then
                    local n = v.Name:lower()
                    if n:find("rank") or n:find("tag") or n:find("admin") or n:find("chat") or n:find("title") or n:find("name") then
                        table.insert(foundRemotes, v)
                    end
                end
            end
            
            for _, remote in ipairs(foundRemotes) do
                task.spawn(function()
                    pcall(function()
                        if remote:IsA("RemoteEvent") then
                            -- Mass-fire common arguments to "force" the server to accept the change
                            remote:FireServer("Rank", rank)
                            remote:FireServer("SetRank", rank)
                            remote:FireServer("Admin", rank)
                            remote:FireServer("Tag", "["..rank.."]")
                            remote:FireServer("SetTag", "["..rank.."]")
                            remote:FireServer("UpdateTag", "["..rank.."]")
                            remote:FireServer("Title", "["..rank.."]")
                            remote:FireServer("NameTag", "["..rank.."]")
                            -- Try firing with player object and userId as well
                            remote:FireServer(player, rank)
                            remote:FireServer(player.UserId, rank)
                        else
                            remote:InvokeServer("Rank", rank)
                            remote:InvokeServer("SetRank", rank)
                            remote:InvokeServer("Tag", "["..rank.."]")
                        end
                    end)
                end)
            end
        end

