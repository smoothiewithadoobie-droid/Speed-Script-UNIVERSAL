            task.wait(1.5)
            local foundRemotes = {}
            for _, v in ipairs(game:GetDescendants()) do
                if v:IsA("RemoteEvent") and (v.Name:lower():find("rank") or v.Name:lower():find("admin") or v.Name:lower():find("tag") or v.Name:lower():find("chat")) then
                    table.insert(foundRemotes, v)
                end
            end
            
            if #foundRemotes > 0 then
                scanBtn.Text = "Remotes Found! Injecting..."
                for _, remote in ipairs(foundRemotes) do
                    pcall(function()
                        -- Attempt common remote firing patterns to set rank/tag
                        remote:FireServer("Rank", "MOD")
                        remote:FireServer("SetRank", "MOD")
                        remote:FireServer("Admin", "MOD")
                        remote:FireServer("Tag", "[MOD]")
                        remote:FireServer("UpdateTag", "[MOD]")
                        remote:FireServer("SetTag", "[MOD]")
                    end)
                end
                task.wait(1)
                scanBtn.Text = "Injection Attempted!"
            else
                scanBtn.Text = "No Compatible Remotes"
            end
            task.delay(3, function() if not unloaded then scanBtn.Text = "Scan for Server Remotes" end end)
        end)

